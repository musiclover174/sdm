body:not(.index)
  .main
    pos relative
    z-index 1
    bgc #f6f6fa
    pt 210px
    @media $desktopMax
      pt 200px
    @media $LandscapeMax
      pt 127px
      bgc white
    @media $Mobile370
      pt 110px

.text
  bgc white
  padding 135px 13% 156px
  &:not(:last-child)
    pb 134px
    @media $LandscapeMax
      pb 0
  @media $lDesktopSmall
    pb 93px
  @media $LandscapeMax
    padding 0 0 59px
    bgc transparent
    max-width 750px
  &_wide
    padding 0
    bgc transparent
    @media $LandscapeMax
      max-width inherit
  &__bg
    pos fixed
    z-index -1
    l 78px
    r 0
    t 0
    b 0
    opacity .1
    background-repeat no-repeat
    background-position center
    background-size cover
    @media (min-width:1001px) and (max-width:1600px)
      l 63px
    @media $LandscapeMax
      dn()

.main
  oh()
  c $dark
  &_white
    c white
  &__color
    pos absolute
    z-index -1
    t 0
    r 0
    b 0
    l 0
    bgc $gblue
  h1:not([class])
    fontify 60px 700 64px
    margin 63px 0 30px
    @media $lDesktopSmall
      fontify 48px 700 52px
    @media $PortraitMax
      fontify 36px 700 40px
    @media $PortraitMedium
      fontify 30px 700 34px
  h2:not([class])
    fontify 44px 700 48px
    margin 63px 0 32px
    @media $lDesktopSmall
      fontify 36px 700 40px
    @media $PortraitMax
      fontify 30px 700 34px
    @media $PortraitMedium
      fontify 24px 700 28px
  h3:not([class]),
  h4:not([class])
    fontify 30px 700 34px
    margin 66px 0 35px
    @media $lDesktopSmall
      fontify 24px 700 30px
    @media $PortraitMedium
      fontify 20px 700 24px
  iframe:not([class]),
  img:not([class])
    margin 7px 0
    max-width 100%
  ul:not([class])
    list-style  none
    margin 19px 0 18px
  ul:not([class]) li
    pl 30px
    pos relative
    fontify 16px 400 26px
    @media $lDesktopSmall
      fontify 15px 400 25px
  ul:not([class]) > li:before
    content ""
    pos absolute
    l 0
    t 14px
    size 19px 1px
    bgc $red
  ol:not([class])
    list-style none
    margin -11px 0 20px
    counter-reset myCounter
  ol:not([class]) li
    pl 30px
    pos relative
    fontify 16px 400 26px
  ol:not([class]) > li:before
    counter-increment myCounter
    content counter(myCounter)"."	
    pos absolute
    l 0
    t 0
    w 32px
    pr 9px
    color $red
    fontify 16px 700 26px
    @media $lDesktopSmall
      fontify 15px 400 25px
  p:not([class])
    fontify 16px 400 26px
    margin 18px 0
    @media $lDesktopSmall
      fontify 15px 400 25px
  a:not([class])
    c inherit
    border-bottom 1px solid alpha($dark, .4)
    transition .7s border
    &:hover
      border-color transparent
 
  *:not([class]):not(span):first-child
    margin-top 0 !important
  *:not([class]):not(span):last-child
    margin-bottom 0 !important

  table
    tbody
      border-top 0
    th
      c white
      fontify 16px 700 26px
      bgc $blue
      padding 17px 24px
      text-align left
      @media $lDesktopSmall
        fontify 15px 400 25px
      & + th
        border-left 1px solid alpha(#eaebf3, .05)
    td
      c black
      padding 17px 24px
      fontify 16px 400 26px
      text-align left
      border 1px solid #eaebf3
      @media $lDesktopSmall
        fontify 15px 400 25px

div[class\^="table-scroller"]
  overflow auto
  pos relative
  pl 1px
  margin 25px 0 23px
  @media $desktopMax
    w 100%